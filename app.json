{
  "expo": {
    "name": "WinDeck Client",
    "slug": "WinDeck-client",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/windeck-icon.png",
    "scheme": "windeckclient",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/windeck-icon.png",
        "backgroundColor": "#2e2d2d"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.devdaru.WinDeckclient"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bfcb1d81-3e53-475e-85d2-d79728dd8db0"
      }
    }
  }
}
